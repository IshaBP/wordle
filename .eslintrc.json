{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "react-app"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": "off",
    "no-var": "error",
    "prefer-const": "error"
  }
}
